#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#define ARRAY_SIZE 8


bool compare(int array1[], int array2[])
{
    int equal_arrays = 1;
    
    for (int i = 0; i < array1[ARRAY_SIZE]; i++){
        for (int i = 0; i < array2[ARRAY_SIZE]; i++){
            if (array1[i] != array2[i]){
                equal_arrays = 0;
                break;
                }
        }
    }
    return equal_arrays;
}
      
int main(){
    
    int array1[ARRAY_SIZE] = {0, 0, 0, 0, 0, 0, 0, 0};
    int array2[ARRAY_SIZE] = {0, 0, 0, 0, 0, 0, 0, 0};
    int a;
    
    a = compare(array1, array2);
    
    if (a == true){
        printf("succes");
    } else{
        printf("failed");
    }
    return 0;    
}
